package project;

public class Main {
    public static int[] makeArray(int n){
    int[] t=new int[n];
    for(int i=0; i<n; i++){
        t[i]=i; //0~3까지 배열 저장
    }
    return t;
}

    public static void main(String[] args) {
        int[] a=makeArray(4);
        for(int i=0; i<a.length; i++)
        System.out.print(a[i]+" "); 
    }
}
