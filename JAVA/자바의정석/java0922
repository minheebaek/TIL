public static void main(String[] args) {//복습
		//add 메서드 만들기
		int num1 =10;
		int num2 =20;
		
		//add 메서드 호출하기
		int result=add(num1,num2);
		System.out.println(result);
		
		
	}
	public static int add(int n1, int n2) { //static 붙이기, 객체를 이용하지 않으니까
		int sum=n1+n2;
		return sum;
	}
  
  public class FuntionTestJava {

	public static void main(String[] args) {
		int num1 = 10;
		int num2 = 25;

		int addresult = add(num1, num2);
		int minusresult = minus(num1, num2);
		double divideresult = divide(num1, num2);
		int multiresult = multi(num1, num2);
		System.out.println(addresult);
		System.out.println(minusresult);
		System.out.println(divideresult);
		System.out.println(multiresult);
	}

	public static int add(int n1, int n3) {
		int sum = n1 + n3;
		return sum;
	}

	public static int minus(int n1, int n3) {
		int sum = n1 - n3;
		return sum;
	}

	public static double divide(int n1, int n3) {
		double sum = (double) n1 / n3;
		return sum;
	}

	public static int multi(int n1, int n3) {
		int sum = n1 * n3;
		return sum;
	}
}

