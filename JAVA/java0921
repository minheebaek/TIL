public class Person{
  String name;
  int age;
  char gender;
  }
  
  //Person 클래스 만들기
  
  package classpart;
  
  public class Person{
   String name;
   int height;
   double weight;
   char gender;
   boolean married;
   }
   
   //학생 이름과 주소 출력하는 메서드 만들기
   package classpart;
   
   public class Student{
    int studentID;
    String studentName;
    int grade;
    String address;
    
    public void showStudentInfo(){ //이름 출력 메서드 
      System.out.println(studentName+","+address);
    }
  }
  
  package classpart;
  public class FunctionTest{
    public static void main(String[ ] args){
    int num1=10;
    int num2=20;
    
    int sum = add(num1, num2);
    System.out.println(num1+"+"+num2+"="+sum+"입니다");
    }
    public static int add(int n1, int n2){
    int result = n1+n2;
    return result;
    }
    }
