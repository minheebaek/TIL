package chapter3;

public class 연습8 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[][] arry = new int[3][5];
		for (int i = 0; i < 3; i++) { 
			for (int j = 0; j < 5; j++) {
				arry[i][j] = (i + 1) + (j * 3); //i부터 풀기, j는 계속 0이니까 방해를 안 받음, i만들고 j만들면 더 수월다고 느낌.
				System.out.print(arry[i][j] + " ");
			}
			System.out.println();
		}

	}

}
