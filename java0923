public class Student { //학생 이름을 반환하는 메서드
  int studentId;
	String studentName;
	int grade;
	String address;
		    
	public String getStudentName() {
		return studentName;
    }
}

public class Student { //학생 이름을 부여하는 메서드

	int studentId;
	String studentName;
	int grade;
	String address;
		
	public String getStudentName() {
		return studentName;

	}
	public void setStudentName(String name) {
		studentName=name;
	}

}

public class Student { //Student 클래스에 main() 함수 추가하기
	int studentId;
	String studentName;
	int grade;
	String address;

	public String getStudentName() {
		return studentName;

	}

	public static void main(String[] args) {
		Student studentAhn = new Student();// 학생 '안' 객체 만듦
		studentAhn.studentName = "안연수";

		System.out.println(studentAhn.studentName);
		System.out.println(studentAhn.getStudentName());
	}

}
